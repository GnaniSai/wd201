<div class="table-content">
<table>
    <tr>
        <th>S.No</th>
        <th>Todo Title</th>
        <th>Due date</th>
    </tr>
    <% for(let i = 0; i<allTodos.length; i++){%>
        <tr>
            <td><%= i+1 %></td>
            <td><%= allTodos[i].title %></td>
            <td><%= allTodos[i].dueDate %></td>
        </tr>
    <% }%>
</table>
</div>